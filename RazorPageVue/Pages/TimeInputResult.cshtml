@page
@model RazorPageVue.Pages.TimeInputResultModel
@{
    ViewData["Title"] = "TextInput Check Result";
}
@addTagHelper *,RazorPageVue

<form method="post">
    <div id="components-demo">
        <h1 class="display-4">時刻入力結果</h1>

        <div class="content box">
            <label>設定なし</label><br />
            <span id="NormalTimeSpan">@Model.NormalTimeString </span>
        </div>
        <div class="content box">
            <label>必須</label><br />
            <span id="RequiredTimeSpan">@Model.RequiredTimeSpan </span>
        </div>
        <div class="content box">
            <label>必須 エラーメッセージ変更</label><br />
            <span id="RequiredTimeSpanChange">@Model.RequiredTimeSpanChange </span>
        </div>
        <hr />

        <div class="content box">
            <label>最小</label><br />
            <span id="MinTimeSpan">@Model.MinTimeSpan </span>
        </div>
        <div class="content box">
            <label>最小 エラーメッセージ変更</label><br />
            <span id="MinErrTimeSpan">@Model.MinErrTimeSpan </span>
        </div>
        <hr />

        <div class="content box">
            <label>最大</label><br />
            <span id="MaxTimeSpan">@Model.MaxTimeSpan </span>
        </div>
        <div class="content box">
            <label>最大 エラーメッセージ変更</label><br />
            <span id="MaxErrTimeSpan">@Model.MaxErrTimeSpan </span>
        </div>
        <hr />

        <div class="content box">
            <label>ステップ</label><br />
            <span id="StepTimeSpan">@Model.StepTimeSpan </span>
        </div>
        <div class="content box">
            <label>ステップ エラーメッセージ変更</label><br />
            <span id="StepErrTimeSpan">@Model.StepErrTimeSpan </span>
        </div>
        <hr />

        <div class="content box">
            <label>最小最大ステップ</label><br />
            <span id="MinMaxStepTimeSpan">@Model.MinMaxStepTimeSpan </span>
        </div>

        <a id="returnLink" href="~/TimeInput">戻る</a>
    </div>
</form>